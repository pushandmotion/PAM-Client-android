<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pushandmotion.pam.MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="PageName" />
    <EditText
        android:id="@+id/textPageName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:text="home" />
    <Button
        android:id="@+id/trackBTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TrackPageView"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="updfh" />
    <EditText
        android:id="@+id/textUPDFH"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:text="@string/mock_updfh" />
    <Button
        android:id="@+id/updfhBTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="set UPDFH"/>


</LinearLayout>
